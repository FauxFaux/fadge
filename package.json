{
  "name": "fadge",
  "version": "0.1.0",
  "description": "parse with babel and check for circular imports",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "prepare": "tsc",
    "format": "prettier --write --log-level=warn 'src/**/*.ts' .babelrc",
    "lint": "eslint 'src/**/*.ts'",
    "start": "node dist/cli.js",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FauxFaux/fadge.git"
  },
  "author": "Chris West (Faux) <solo-npm@goeswhere.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FauxFaux/fadge/issues"
  },
  "homepage": "https://github.com/FauxFaux/fadge#readme",
  "bin": {
    "fadge": "dist/cli.js"
  },
  "dependencies": {
    "@babel/parser": "^7.12.11",
    "@babel/traverse": "^7.12.12",
    "@babel/types": "^7.12.12",
    "fast-glob": "^3.2.5",
    "yargs": "^17.7.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/babel__traverse": "^7",
    "@types/yargs": "^17.0.22",
    "eslint": "^9",
    "prettier": "~3.5",
    "ts-node": "^10.9",
    "typescript": "~5.8",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3"
  }
